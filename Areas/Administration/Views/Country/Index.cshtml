
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_HMSMainLayout.cshtml";
}

<h1>Index</h1>


<form method="post" enctype="multipart/form-data" asp-action="Index" asp-controller="Country" asp-area="Administration">
    <div class="container">

        <div class="col-md-12">
            
            <div class="col-md-4">

              
                        <input type="file" id="postedFile" name="postedFile" class="form-control" accept=".xls,.xlsx" />

                    </div>
               
               
                    <div class="col-md-3">
                        <br />
                        <input type="submit" title="Upload" value="Upload" class="btn btn-success" />
                    </div>
               </div>
                <div class="form-group">

                    <div class="col-md-offset-3 col-md-10">
                        <br />
                        <span style="color:red;">
                            @ViewBag.Message
                        </span>
                    </div>
                </div>
           
        </div>
    

</form>
         